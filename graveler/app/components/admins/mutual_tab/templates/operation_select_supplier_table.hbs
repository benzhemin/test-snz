<table id="range-supplier-table" class="table table-striped table-small">
  <thead>
    <tr>
      <th width="160">供应商</th>
      <th width="108"><a href="javascript:void(0);" class="js-range-supplier range-supplier">C 成本&nbsp;&nbsp;&nbsp;<i data-sort-type="5" class="icon-arrowdown"></i></a></th>
      <th width="150">整体方案</th>
    </tr>
  </thead>
  <tbody data-id="{{requirementId}}">
    {{#each data}}
      <tr class="supplier-solution-tr">
        <td>{{supplierName}}</td>
        <td>{{formatPrice cost}}</td>
        <td class="file-show-td" data-file="{{solutionFile}}"></td>
      </tr>
    {{/each}}
  </tbody>
</table>
