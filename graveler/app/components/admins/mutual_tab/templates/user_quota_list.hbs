<div class="table-container">
	<table class="table table-striped table-small">
    <thead>
      <tr>
        <th width="100">模块编号</th>
        <th width="200">物料描述</th>
        <th width="100">工厂</th>
        <th width="200">供应商名称</th>
        <th width="200">配额</th>
        <th width="100">配额比例</th>
      </tr>
    </thead>
    <tbody>
      {{#each data.data}}
      <tr class="quota-tr">
        <td>{{moduleId}}</td>
        <td>{{moduleName}}</td>
        <td>{{factoryNum}}</td>
        <td>{{supplierName}}</td>
        <td>{{quantity}}</td>
        <td>{{scale}}%</td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>